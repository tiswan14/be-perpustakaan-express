generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id       String  @id @default(cuid())
  nama     String
  email    String  @unique
  nim      String?
  nid      String?
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
